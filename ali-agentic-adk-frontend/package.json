{
  "name": "ali-agentic-adk-frontend",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "prepare": "husky",
    "reinstall": "pnpm -r exec del node_modules .turbo src/.umi* dist logs run && del .turbo && pnpm install",
    "start": "docker-compose -f docker-compose.yml -f docker-compose-db.yml --env-file ./server/.env up --build -d",
    "start:db": "docker-compose -f docker-compose-db.yml --env-file ./server/.env up -d"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@umijs/lint": "^4.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stylelint": "^15.0.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "20.18.0",
    "pnpm": "9.15.0"
  }
}
